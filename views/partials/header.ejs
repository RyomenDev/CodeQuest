<script src="https://cdn.jsdelivr.net/npm/typed.js@2.0.12"></script>

<script src="https://cdn.jsdelivr.net/npm/gsap"></script>

<script>
    gsap.fromTo(
        ".header-laptop-1",
        {x: -800, rotation: 40},
        {x: 0, rotation: 0, duration: 2, ease: "expo.out"}
    );
    gsap.fromTo(
        ".header-laptop-4",
        {x: 2000, rotation: -60},
        {x: 0, rotation: 0, duration: 2, delay: 1, ease: "expo.out"}
    );
    gsap.fromTo(
        ".header-laptop-5",
        {y: -800},
        {y: 0, duration: 2, delay: 0.5, ease: "expo.out"}
    );
    gsap.fromTo(
        ".header-laptop-6",
        {y: -800},
        {y: 0, duration: 2, delay: 2, ease: "expo.out"}
    );
    gsap.fromTo(
        ".header-laptop-2",
        {y: -800, rotation: 30},
        {y: 0, rotation: 0, duration: 2, delay: 2, ease: "bounce.out"}
    );
</script>

<script>
    let typed = new Typed(".auto-type", {
        strings: ["Coding^500", "Sleeping^500", "Eating^500"],
        typeSpeed: 150,
        backSpeed: 150,
        loop: true,
        cursorChar: "_",
    });
</script>